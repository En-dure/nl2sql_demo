2024-08-23 15:02:32,879 - INFO ----- sql:WITH 张立国医生腰椎手术 AS (
    SELECT
        主手术代码,
        主手术名称 AS 病种,
        带组医师 AS 主刀医生,
        带组医师工号,
        COUNT(*) AS 例数,
        SUM(总费用) AS 总费用,
        SUM(总药费) AS 总药费,
        SUM(总材料费) AS 总卫生材料费,
        SUM(住院天数) AS 住院天数
    FROM
        病历记录
    WHERE
        带组医师 = '张立国'
        AND `主手术代码` IN ("80.5100x033","80.5100x034","81.0401","81.6600x002","81.6600x003")
        AND 出院科室 IN ("骨科一区","骨科二区","骨科三区","骨科四区","骨科五区","骨科六区","肌骨超声诊疗中心")
        AND 出院日期 BETWEEN '2023-01-01' AND '2023-11-30'
    GROUP BY
        带组医师,
        带组医师工号,
        主手术代码,
        主手术名称
)
SELECT
    主刀医生,
    带组医师工号,
    SUM(例数) AS 例数,
    SUM(总费用) / SUM(例数) AS 均次费,
    SUM(总药费) / SUM(例数) AS 均次药费,
    SUM(总药费) / SUM(总费用) AS 药占比,
    SUM(总卫生材料费) / SUM(例数) AS 均次卫生材料费,
    SUM(总卫生材料费) / SUM(总费用) AS 耗占比,
    (SUM(总费用) - SUM(总药费) - SUM(总卫生材料费)) / SUM(总费用) AS 去药去耗材占比,
    SUM(住院天数) / SUM(例数) AS 平均住院日
FROM 张立国医生腰椎手术
GROUP BY 主刀医生,
    带组医师工号;
