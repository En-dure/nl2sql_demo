SELECT COUNT(DISTINCT b.病案号) AS 出院人数
FROM 病历记录 b
WHERE b.出院日期 BETWEEN '2023-01-01' AND '2023-11-30'
AND b.所属病区 IN ('骨科一区', '骨科二区', '骨科三区', '骨科四区', '骨科五区', '骨科六区', '肌骨超声诊疗中心')

UNION ALL

-- 手术例数
SELECT COUNT(DISTINCT b.病案号) AS 手术例数
FROM 病历记录 b
WHERE b.出院日期 BETWEEN '2023-01-01' AND '2023-11-30'
AND b.所属病区 IN ('骨科一区', '骨科二区', '骨科三区', '骨科四区', '骨科五区', '骨科六区', '肌骨超声诊疗中心')
AND b.主手术代码 IS NOT NULL

UNION ALL

-- 出院患者手术台次数
SELECT COUNT(b.主手术代码) AS 出院患者手术台次数
FROM 病历记录 b
WHERE b.出院日期 BETWEEN '2023-01-01' AND '2023-11-30'
AND b.所属病区 IN ('骨科一区', '骨科二区', '骨科三区', '骨科四区', '骨科五区', '骨科六区', '肌骨超声诊疗中心')
AND b.主手术代码 IS NOT NULL

UNION ALL

-- 出院患者手术占比
SELECT COUNT(b.主手术代码) * 100.0 / (SELECT COUNT(DISTINCT 病案号) FROM 病历记录 WHERE 出院日期 BETWEEN '2023-01-01' AND '2023-11-30' AND 所属病区 IN ('骨科一区', '骨科二区', '骨科三区', '骨科四区', '骨科五区', '骨科六区', '肌骨超声诊疗中心')) AS 出院患者手术占比
FROM 病历记录 b
WHERE b.出院日期 BETWEEN '2023-01-01' AND '2023-11-30'
